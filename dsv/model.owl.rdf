    <rdf:type rdf:resource="http://www.w3.org/ns/dx/prof/Profile"/>
    <dcterms:title xml:lang="en">Data Specification Vocabulary</dcterms:title>
    <dcterms:description xml:lang="en">Data Specification Vocabulary (DSV) is a vocabulary for describing semantic data specifications, namely vocabularies and application profiles.</dcterms:description>
    <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date"
    >2024-10-01</dcterms:issued>
