  <owl:Ontology rdf:about="https://w3id.org/dsv#">
    <foaf:depiction rdf:resource="https://mff-uk.github.io/data-specification-vocabulary/dsv/c5d2ee2e-32c6-4c12-abb3-b80410162920.svg"/>
